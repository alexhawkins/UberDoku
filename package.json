{
    "name": "UberDoku",
    "version": "1.0.0",
    "description": "A Sedoku Board Game in the style of Uber",
    "main": "app.module.es6",
    "scripts": {
        "build": "./node_modules/.bin/webpack -d",
        "watch": "./node_modules/.bin/webpack -d --watch",
        "server": "./node_modules/.bin/webpack-dev-server -d --inline --colors --display-error-details --content-base public",
        "hot": "NODE_ENV=hot-dev webpack -d",
        "start": "npm install && npm run build && npm run server",
        "hotbox": "npm install && npm run hot && npm run server ",
        "test": "echo \"Error: no test specified\" && exit 1",
        "deploy": "NODE_ENV=production webpack -p"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/alexhawkins/UberDoku.git"
    },
    "keywords": [
        "uber",
        "es6",
        "JavaScript",
        "sass",
        "sedoku",
        "jquery",
        "modular"
    ],
    "author": "Alex Hawkins 2015",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/alexhawkins/UberDoku/issues"
    },
    "homepage": "https://github.com/alexhawkins/UberDoku",
    "dependencies": {
        "es6-promise": "^2.0.1",
        "jquery": "^2.1.3",
        "lodash": "^3.6.0"
    },
    "devDependencies": {
        "autoprefixer-loader": "^1.2.0",
        "babel-core": "^5.0.8",
        "babel-loader": "^5.0.0",
        "css-loader": "^0.9.1",
        "extract-text-webpack-plugin": "^0.5.0",
        "file-loader": "^0.8.1",
        "jshint": "^2.6.3",
        "jshint-loader": "^0.8.3",
        "node-sass": "sass/node-sass#v3.0.0-alpha.1",
        "raw-loader": "^0.5.1",
        "sass-loader": "^0.4.2",
        "style-loader": "^0.9.0",
        "webpack": "^1.7.3",
        "webpack-dev-server": "^1.8.0"
    }
}
